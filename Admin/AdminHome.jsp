<SCRIPT>

 var i=0;
 var nTips=75;

 function Load() 
 {
	var d = new Date();
	i =  ((d.getMonth() * 31) + d.getDate()) %nTips;
	if (i==null) i=0;
		eval("document.all.Tip"+i+".style.display=''");
 }

 function goNext()
 {
	eval("document.all.Tip"+i+".style.display='none'");
	i=(((i%nTips)+1)%nTips);
	eval("document.all.Tip"+i+".style.display=''");
 }

 function showName() 
 {
	window.status = event.srcElement.innerText;
	return true;
 }

</SCRIPT>
